{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "es2019",
        "noImplicitAny": false,
        "strict": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "useUnknownInCatchVariables": false,
        "strictNullChecks": false
    }
}